{
  "name": "nacos-sdk-nest",
  "version": "1.0.0-alpha.0",
  "description": "Nacos SDK for Nestjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"*.ts\"",
    "lint": "eslint '**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "publish": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xajsrain/nacos-sdk-nest.git"
  },
  "keywords": [
    "nacos",
    "nestjs",
    "sdk"
  ],
  "files": [
    "dist"
  ],
  "author": "xajsrain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xajsrain/nacos-sdk-nest/issues"
  },
  "homepage": "https://github.com/xajsrain/nacos-sdk-nest#readme",
  "devDependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^8.4.7",
    "@nestjs/testing": "^8.4.7",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.3",
    "prettier": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
